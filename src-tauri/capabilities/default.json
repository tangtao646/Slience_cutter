{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:default",
    "fs:default",
    "fs:allow-temp-read-recursive",
    "fs:allow-temp-write-recursive",
    "fs:allow-home-read-recursive",
    "fs:allow-download-read-recursive",
    "dialog:default"
  ]
}
